      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

  </head>

<body>

<div class="navbar navbar-fixed-top">
	
	<div class="navbar-inner">
		
		<div class="container">
			
			<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</a>
			
			<a class="brand" href="/">
				Складская логистика | Админ панель				
			</a>		
			
			<div class="nav-collapse">
				<ul class="nav pull-right">
					
			
					<li class="dropdown">						
						<a href="#" class="dropdown-toggle" data-toggle="dropdown">
							<i class="icon-user"></i> 
							{{login}}
							<b class="caret"></b>
						</a>
						
						<ul class="dropdown-menu">
							<li><a href="?logout=1">Logout</a></li>
						</ul>						
					</li>
				</ul>
			
				
			</div><!--/.nav-collapse -->	
	
		</div> <!-- /container -->
		
	</div> <!-- /navbar-inner -->
	
</div> <!-- /navbar -->
    



    
<div class="subnavbar">

	<div class="subnavbar-inner">
	
		<div class="container">

			<ul class="mainnav">
			
		   <li ><a href="/"><i class="icon-dashboard"></i><span>Главная</span> </a> </li>
       {%if role !=1%} <li ><a href="/tasks"><i class="icon-list-alt"></i><span>Задания</span> </a> </li>{%endif%}
        <li class="active" ><a href="/sklad"><i class="icon-bar-chart"></i><span>Склад</span> </a> </li>
		 {%if role !=1%}<li  ><a href="/yprav"><i class=" icon-asterisk"></i><span>Управление</span> </a> </li>{%endif%}
		 <li  ><a href="/transaction"><i class="icon-align-justify"></i><span>Транзакции</span> </a> </li>
		 {%if role !=2 and role !=3%} <li   ><a href="/users"><i class="icon-user"></i><span>Пользователи</span> </a> </li>{%endif%}
			</ul>

		</div> <!-- /container -->
	
	</div> <!-- /subnavbar-inner -->

</div> <!-- /subnavbar -->
    

    
<div class="main">
	
	<div class="main-inner">

	    <div class="container">
	    	
	     <div class="row">
	      	
	      	<div class="span12">
	      
	      	<div class="info-box">

               <div class="row-fluid stats-box">
                  <div class="span4">
                  	<div class="stats-box-all-info">Сектор {{name}}</div>
					  
            {% for product in product%}      <div class="stats-box-title"><img src="/web/img/korobka1.png" height="100px" width="100px">  {{product.quarty}}/{{product.countY}}</div>    {% endfor %}
					{%if not product%}  <div class="stats-box-title"><img src="/web/img/korobka1.png" height="100px" width="100px">  N/N </div> {%endif%}
                   </div>
		   




                  <div class="span4">
                    <div class="stats-box-all-info">Описание</div>
				{% for product in product%}
					 <div class="stats-box-title"><i class="icon-filter"  style="color:orange"></i> тип: {{product.name}}</div>
                    <div class="stats-box-title"><i class="icon-pencil"  style="color:#F30"></i> размер: {{product.size}}</div>
					<div class="stats-box-title"><i class="icon-adjust"  style="color:blue"></i> цвет: {{product.color}}</div>
					<div class="stats-box-title"><i class="icon-reorder"  style="color:green"></i> конфигурация: {{product.config}}</div>
                {% endfor %}
{%if not product%} <div class="stats-box-title"><i class="icon-filter"  style="color:orange"></i> тип: Пустой</div>{%endif%}
                   </div>
                 
				 <div class="span4">
                    <div class="stats-box-all-info">Заполнение</div>
                    <div class="controls">
				<div class="progress progress-striped active" style="width: 95%;">
	
                     {% for product in product%}         <div class="bar" style="width: {{product.quarty/product.countY*100}}%; background-color:green;"></div>{% endfor %}
{%if not product%} <div class="bar" style="width: 0%; background-color:green;"></div>{%endif%}
							
                                 </div>
					</div> 
                   </div>
               </div>
               
           
             </div>
               
               
         </div>
         </div>      
	      	
	  	  <!-- /row -->
	
	      <div class="row">
	      	{%if not product%}
				
				{%else%}
	      	<div class="span6">
	      		
	      		<div class="widget">
						
					<div class="widget-header">
						<i class="icon-star"></i>
						<h3>Ячейка относительно других</h3>
					</div> <!-- /widget-header -->
					
					<div class="widget-content">
						<canvas id="pie-chart" class="chart-holder" height="250" width="538"></canvas>
					</div> <!-- /widget-content -->
						
				</div> <!-- /widget -->
				
	      		
	      		
	      		
		    </div> <!-- /span6 -->
	      
	      	{%if role !=1%}
	      	<div class="span6">
	      		
	      		<div class="widget">
							
					<div class="widget-header">
						<i class="icon-list-alt"></i>
						<h3>Действия</h3>
					</div> <!-- /widget-header -->
					
					<div class="widget-content">
					Внутрескладское перемещение:<p>
					  <table class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th> Количество коробок</th>
                    <th> В</th>
                    <th class="td-actions"></th>
                  </tr>
                </thead>
                <tbody>
				<form action="/yacheika/{{name}}" method="post">
                  <tr>
				  <input name="yacheikaout" value="{{name}}" style='display:none'></input>
                    <td> <input name="kol" placeholder="10" value="{{kolichestvo}}"></input> </td>
                    <td> <input name="yacheika" value="{{sklad}}.{{nameyach}}"></input></td>
                    <td class="td-actions">
					
					<button class="btn btn-small btn-success"  ><i class="btn-icon-only icon-wrench"> </i></button>
					</td>
                  </tr>
                 </form>
                 
                
                </tbody>
              </table>
			 
					</div> <!-- /widget-content -->
				
				</div> <!-- /widget -->
									
		      </div> <!-- /span6 -->
	      		{% endif %}
	      </div> <!-- /row -->
	      
	      
	      
	      {%endif%}
			
	      
	      
	    </div> <!-- /container -->
	    
	</div> <!-- /main-inner -->
    
</div> <!-- /main -->
    

    

<div class="extra">

	<div class="extra-inner">

		<div class="container">

			<div class="row">
                   
                </div> <!-- /row -->

		</div> <!-- /container -->

	</div> <!-- /extra-inner -->

</div> <!-- /extra -->


    
    
<div class="footer">

	<div class="footer-inner">
		
		<div class="container">
			
			<div class="row">
				
    			<div class="span12"> &copy; 2018 <a href="#">Складская логистика</a>. </div>
    			
    		</div> <!-- /row -->
    		
		</div> <!-- /container -->
		
	</div> <!-- /footer-inner -->
	
</div> <!-- /footer -->
    

<script src="/web/js/jquery-1.7.2.min.js"></script>
<script src="/web/js/excanvas.min.js"></script>
<script src="/web/js/chart.min.js" type="text/javascript"></script>
<script src="/web/js/bootstrap.js"></script>
<script src="/web/js/base.js"></script>
<script>

    var pieData = [
				{
				    value: {% for product in product%} {{product.quarty}} {% endfor %},
				    color: "#F38630"
				},
				{
				    value: {{summa}},
				    color: "#69D2E7"
				}

			];

    var myPie = new Chart(document.getElementById("pie-chart").getContext("2d")).Pie(pieData);

    var barChartData = {
        labels: ["January", "February", "March", "April", "May", "June", "July"],
        datasets: [
				{
				    fillColor: "rgba(220,220,220,0.5)",
				    strokeColor: "rgba(220,220,220,1)",
				    data: [65, 59, 90, 81, 56, 55, 40]
				},
				{
				    fillColor: "rgba(151,187,205,0.5)",
				    strokeColor: "rgba(151,187,205,1)",
				    data: [28, 48, 40, 19, 96, 27, 100]
				}
			]

    }

    var myLine = new Chart(document.getElementById("bar-chart").getContext("2d")).Bar(barChartData);
	var myLine = new Chart(document.getElementById("bar-chart1").getContext("2d")).Bar(barChartData);
	var myLine = new Chart(document.getElementById("bar-chart2").getContext("2d")).Bar(barChartData);
	var myLine = new Chart(document.getElementById("bar-chart3").getContext("2d")).Bar(barChartData);
	
	</script>
   
 
