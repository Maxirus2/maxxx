<div class="tn-box tn-box-color-3" style="
 bottom:0;
 right:0;
 position: fixed;
  z-index: 101; 
  opacity:0.5;
 ">
					<p>{{error}}<p>
					<div class="tn-progress"></div>
				</div>
                

{%endif%}

{%if asecc!=""%}

              	<div class="tn-box tn-box-color-2" style="
 bottom:0;
 right:0;
 position: fixed;
  z-index: 101;
  opacity:0.5;
 ">
					<p>{{asecc}}</</p>
					<div class="tn-progress"></div>
				</div>  

{%endif%}
  </body>

</html>
